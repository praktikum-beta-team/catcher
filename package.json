  "scripts": {
    "dev": "webpack --config config/webpack/server/webpack.dev.ts",
    "build:client": "webpack --config config/webpack/client/webpack.prod.ts --json=dist/compilation-stats.json",
    "build:server": "webpack --config config/webpack/server/webpack.prod.ts",
    "build": "npm run build:client && npm run build:server",
    "start": "NODE_ENV=production node dist/server",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "docker:build": "docker build -t catcher .",
    "docker:run": "docker run -it --rm -e PORT=8000 -p 8000:8000/tcp catcher",
    "ssl:dev": "cd config/ssl && openssl req -x509 -out localhost.crt -keyout localhost.key -newkey rsa:2048 -nodes -sha256 -subj '/CN=localhost' -extensions EXT -config 'openssl.cnf'"
